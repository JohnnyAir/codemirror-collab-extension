{
  "name": "collab-extension",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "packageManager": "pnpm@8.11.0",
  "namespace": "johncodes",
  "devDependencies": {
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "scripts": {
    "clean": "pnpm --filter \"{lib,demo}/**/*\" run clean",
    "build": "pnpm -r build",
    "prettier": "prettier --ignore-unknown \"{lib,demo}/**/*\"",
    "prettier:write": "pnpm run prettier --write"
  }
}
